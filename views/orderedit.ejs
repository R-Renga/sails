<ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/order/orderlist">orders</a></li>
    <li class="breadcrumb-item active">Edit</li>
</ol>
<h1 class="display-4">Edit Order</h1>
<form action="/order/orderupdate?id=<%= order._id %>" method="post">
    <div class="form-group">
        <label for="status">status</label>
        <input type="text" name="status" id="status" class="form-control" value="<%= order.status %>">
    </div>
    <div class="form-group">
        <label for="name">name</label>
        <input type="text" name="name" id="name" class="form-control" value="<%= order.name %>">
    </div>
    <div class="form-group">
        <label for="qty">qty</label>
        <input type="text" name="qty" id="qty" class="form-control" value="<%= order.qty %>">
    </div>
    <div class="form-group">
        <label for="total">total</label>
        <input type="text" name="total" id="total" class="form-control" value="<%= order.total %>">
    </div>
    <input type="submit" value="Submit" class="btn btn-primary">
</form>